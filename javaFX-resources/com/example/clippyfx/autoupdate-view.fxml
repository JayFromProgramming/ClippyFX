<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="213.0" prefWidth="521.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clippyfx.AutoUpdateView">
   <children>
      <Label alignment="CENTER" layoutX="74.0" layoutY="48.0" prefHeight="17.0" prefWidth="373.0" text="A new version of ClippyFX is available, would you like to update?" />
      <Label fx:id="version_bar" alignment="CENTER" layoutX="74.0" layoutY="80.0" prefHeight="17.0" prefWidth="373.0" text="Loading -&gt; Loading" />
      <Hyperlink fx:id="info_link" alignment="CENTER" layoutX="74.0" layoutY="106.0" prefHeight="23.0" prefWidth="373.0" text="More info can be found here" />
      <Button fx:id="update_cancel" layoutX="421.0" layoutY="158.0" mnemonicParsing="false" onMouseClicked="#cancel_clicked" text="Cancel" />
      <Button fx:id="update_confirm" layoutX="48.0" layoutY="158.0" mnemonicParsing="false" onMouseClicked="#update_clicked" text="Update" />
   </children>
</AnchorPane>
