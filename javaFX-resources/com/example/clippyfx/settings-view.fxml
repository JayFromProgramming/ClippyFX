<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pain" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clippyfx.SettingsView">
   <children>
      <ButtonBar layoutX="301.0" layoutY="350.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="saveButton" mnemonicParsing="false" onMouseClicked="#onSavePressed" text="Save" />
            <Button fx:id="resetButton" mnemonicParsing="false" onMouseClicked="#onResetPressed" text="Reset" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onMouseClicked="#onCancelPressed" text="Cancel" />
        </buttons>
      </ButtonBar>
      <Accordion layoutX="14.0" layoutY="14.0" />
      <Accordion fx:id="accordion" layoutX="14.0" layoutY="14.0" prefWidth="574.0" />
   </children>
</AnchorPane>
